{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "main"
      },
      {
         "source": "**",
         "destination": "/index.html"
      }
    ]
  }
}
